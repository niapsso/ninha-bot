{
  "name": "ninha-bot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --inspect --ignore-watch node_modules ./src/index.ts",
    "start": "node ./dist/index.js",
    "deploy": "cross-env IS_SCRIPT=true ts-node ./src/scripts/deploy.ts ",
    "deploy-prod": "cross-env NODE_ENV=prod yarn deploy"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.2.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
